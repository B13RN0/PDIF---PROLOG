nazwisko(biernacka,barbara).


%relacje


syn(X,Y) :-
    mezczyzna(Y),
    rodzic(X,Y).

corka(X,Y) :-
    kobieta(Y),
    rodzic(X,Y).

matka(X,Y) :-
    kobieta(X),
    rodzic(X,Y).

ojciec(X,Y) :-
    mezczyzna(X),
    rodzic(X,Y).

babcia(Z,Y) :-
    rodzic(X,Y),
    kobieta(Z),
    rodzic(Z,X).

dziadek(Z,Y) :-
    rodzic(X,Y),
    rodzic(Z,X),
    mezczyzna(Z).

nazwisko_matki(Z,Y) :-
    rodzic(X,Y),
    kobieta(X),
    nazwisko(Z,X).

nazwisko_ojca(Z,Y) :-
    rodzic(X,Y),
    mezczyzna(X),
    nazwisko(Z,X).

nazwisko_babci(N,Y) :-
    rodzic(X,Y),
    rodzic(Z,X),
    kobieta(Z),
    nazwisko(N,Z).



