%relacje

maz(M,Y) :-
    rodzic(Y,X),
    rodzic(M,X),
    mezczyzna(M).

zona(M,Y) :-
    rodzic(Y,X),
    rodzic(M,X),
    kobieta(M).

brat(Z,Y) :-
    rodzic(X,Y),
    rodzic(X,Z),
    Z\=Y,
    mezczyzna(Z).

siostra(Z,Y) :-
    rodzic(X,Y),
    rodzic(X,Z),
    Z\=Y,
    kobieta(Z).

syn(X,Y) :-
    rodzic(X,Y),
    mezczyzna(Y).

corka(X,Y) :-
    rodzic(X,Y),
    kobieta(Y).

matka(X,Y) :-
    rodzic(X,Y),
    kobieta(X).

ojciec(X,Y) :-
    rodzic(X,Y),
    mezczyzna(X).

babcia(Z,Y) :-
    rodzic(X,Y),
    rodzic(Z,X),
    kobieta(Z).

dziadek(Z,Y) :-
    rodzic(X,Y),
    rodzic(Z,X),
    mezczyzna(Z).

wujek(Z,Y) :-
    rodzic(X,Y),
    brat(Z,X).

ciocia(Z,Y) :-
    rodzic(X,Y),
    siostra(Z,X).

nazwisko_matki(Z,Y) :-
    matka(X,Y),
    nazwisko(Z,X).

nazwisko_ojca(Z,Y) :-
    ojciec(X,Y),
    nazwisko(Z,X).

nazwisko_babci(N,Y) :-
    babcia(X,Y),
    nazwisko(N,X).

nazwisko_dziadka(N,Y) :-
    dziadek(X,Y),
    nazwisko(N,X).

nazwisko_mamy(N,Y) :-
    matka(X,Y),
    nazwisko(N,X).

nazwisko_taty(N,Y) :-
    ojciec(X,Y),
    nazwisko(N,X).

zawod_matki(Z,Y) :-
    matka(X,Y),
    zawod(Z,X).

zawod_ojca(Z,Y) :-
    ojciec(X,Y),
    zawod(Z,X).

zawod_babci(Z,Y) :-
    babcia(R,Y),
    zawod(Z,R).

zawod_dziadka(Z,Y) :-
    dziadek(R,Y),
    zawod(Z,R).

zawod_brata(Z,Y) :-
    brat(X,Y),
    zawod(Z,X).

zawod_siostry(Z,Y) :-
    siostra(X,Y),
    zawod(Z,X).

cecha_brata(C,Y) :-
    brat(X,Y),
    cecha_szczegolna(C,X).

cecha_siostry(C,Y) :-
    siostra(X,Y),
    cecha_szczegolna(C,X).

cecha_ojca(C,Y) :-
    ojciec(X,Y),
    cecha_szczegolna(C,X).

cecha_matki(C,Y) :-
    matka(X,Y),
    cecha_szczegolna(C,X).

cecha_babci(C,Y) :-
    babcia(X,Y),
    cecha_szczegolna(C,X).

cecha_dziadka(C,Y) :-
    dziadek(X,Y),
    cecha_szczegolna(C,X).
